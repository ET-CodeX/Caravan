<Code>
def compPost_AI075(element, parameters):
  
  if parameters['varanBridge']:
    parameters['AddToSDiasTime'] = True
    element['result']['optional'] =  {'PDO': {'Wr':[], 'Rd':[], 'WrDyn':[], 'RdDyn':[]}}    
    SDIASPDORdAdd(0x12,element)    
    if operator.gt(element['options']['UseSamplePointConfig'], 0):
      SDIASPDOWrAdd(0x0F,element)

</Code>
